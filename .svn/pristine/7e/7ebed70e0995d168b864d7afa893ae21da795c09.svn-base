package com.gobestsoft.common.modular.cms.model;

import lombok.Data;

/**
 * create by gutongwei
 * on 2018/6/15 上午10:50
 */
@Data
public class CmsCourseEntity {

    /**
     * 课程ID
     */
    private int course_id;

    /**
     * 课程类型类型:0：图文，1：音频，2：视频
     */
    private int info_type;

    /**
     * 课程封面图
     */
    private String cover;
    /**
     * 标题
     */
    private String title;
    /**
     * 创建时间
     */
    private String create_time;
    /**
     * 浏览数
     */
    private int preview_num;
    /**
     * 点赞数
     */
    private int thumbs_up_num;
    /**
     * 用户是否点过赞【0：没有】【1：已点】
     */
    private int is_thumbs_up;
    /**
     * 用户是否收藏【0：没有】【1：已收藏】
     */
    private int is_collect;
    /**
     * 课程类型【0:普通课程】【1：系列课程】
     */
    private int type;


    private String create_by;
}
