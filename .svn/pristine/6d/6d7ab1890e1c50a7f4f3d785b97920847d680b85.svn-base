<body>
<div class="container">
   <div class="form-horizontal" id="CancelForm">
      <div class="row" >
        <div class="col-sm-2"></div>
        <div class="col-sm-4">
          <form:text id="deptName" label="工会名称" name="deptName" must="true" value="${deptLegalApply.deptName}" disabled="true"></form:text>
        </div>
        <div class="col-sm-4">
          <form:text id="name" label="法定代表人" name="name" must="true" value="${deptLegalApply.name}" disabled="true"></form:text>
        </div>
      </div>
      <div class="row" >
        <div class="col-sm-2"></div>
        <div class="col-sm-4">
          <form:text id="cerNo" label="证书证号" name="cerNo" must="true" value="${deptLegalApply.cerNo}" disabled="true"></form:text>
        </div>
        <div class="col-sm-4">
          <form:text id="agentName" label="经办人" name="agentName" must="true" value="${deptLegalApply.agentName}" disabled="true"></form:text>
      </div>
    </div>
    
      <div class="row" >
        <div class="col-sm-2"></div>
        <div class="col-sm-8">
          <form:area id="cancellationReason" label="注销原因" name="cancellationReason" must="true" value="${deptLegalApply.cancellationReason}" disabled="true"></form:text>
        </div>
      </div>
      <div class="row" >
        <div class="col-sm-2"></div>
        <div class="col-sm-8">
          <form:area id="cacellationBasis" label="注销依据" name="cacellationBasis" must="true" value="${deptLegalApply.cacellationBasis}" disabled="true"></form:text>
        </div>
      </div>
      <div class="row" >
        <div class="col-sm-2"></div>
        <div class="col-sm-8">
          <form:area id="pDeptFullname" label="隶属工会名称" name="pDeptFullname" must="true" value="${deptLegalApply.pDeptFullname}" disabled="true"></form:text>
        </div>
      </div>
    
    <div class="row" >
        <div class="col-sm-2"></div>
        <div class="col-sm-8">
          <form:area id="assetsHanding" label="资产处理情况" name="assetsHanding" must="true" value="${deptLegalApply.assetsHanding}" disabled="true"></form:text>
        </div>
      </div>
      <div class="row" >
        <div class="col-sm-2"></div>
        <div class="col-sm-8">
          <form:area id="debtHanding" label="债权债务处理情况" name="debtHanding" must="true" value="${deptLegalApply.debtHanding}" disabled="true"></form:text>
        </div>
      </div>
      <div class="row text-center">
      <qx:btn id="close" class="btn btn-warning" onclick="CancelDlg.close()" 
          fa="fa fa-eraser" label="取消"></qx:btn>
      </div>
      <br/>
    </div>
</div>
</body>
<js>
<script type="text/javascript">
/**
 * 初始化表单验证
 */
var CancelDlg = {
   LegalCancelData: {},
   validateFields: {
   }
}
/**
 * 关闭此对话框
 */
 CancelDlg.close = function() {
    parent.layer.close(window.parent.LegalCancel.layerIndex);
}
</script>
</js>
