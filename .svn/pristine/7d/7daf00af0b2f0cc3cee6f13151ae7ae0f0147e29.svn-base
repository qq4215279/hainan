package com.gobestsoft.api.modular.srv.model;

import lombok.Data;

/**
 * 困难帮扶附件属性：医疗帮扶
 * create by gutongwei
 * on 2018/10/18 下午5:14
 */
@Data
public class ApplyStudy {

    /**
     * 姓名
     */
    private String name;

    /**
     * 身份证号
     */
    private String certificateNum;


    /**
     * 年龄
     */
    private Integer age;


    /**
     * 性别：字典：lib_sex
     */
    private String sex;


    /**
     * 是否是本届：字典：lib_is_not
     */
    private String isCurrent;


    /**
     * 手机号
     */
    private String mobile;


    /**
     * 政治面貌，字典：lib_political_status
     */
    private String politicalStatus;


    /**
     *学历：字典：lib_education_code
     */
    private Integer education;


    /**
     * 高考分数
     */
    private Double collegeScore;

    /**
     * 录取学院
     */
    private String enrollSchool;


    /**
     * 入取专业
     */
    private String enrollMajor;

    /**
     * 学制（年）
     */
    private Integer schoolYear;

    /**
     * 录取批次
     */
    private String enrollBatch;


    /**
     * 录取时间（yyyyMMdd）
     */
    private String enrollTime;

    /**
     * 毕业时间（yyyyMMdd）
     */
    private String graduationTime;

    /**
     * 学杂费
     */
    private Double learnMoney;


    /**
     * 是否有助学贷：字典：lib_is_not
     */
    private String hasLoan;

    /**
     * 助贷金额
     */
    private Double loanMoney;


    /**
     *	现家庭居住地址
     */
    private String liveAddress;

    /**
     *	居住详细地址
     */
    private String address;
    /**
     * 民族
     */
    private String nation;
}
