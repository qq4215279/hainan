package com.gobestsoft.common.modular.cms.model;

import java.io.Serializable;

import com.gobestsoft.core.util.DateUtil;

/**
 * 订阅号动态
 * 
 * @author gutongwei
 *
 *         2017年11月29日
 */
public class SubscriptionDynamic {

	private long id;

	private String name;

	private String avatar;

	private String title;

	private String create_time;

	private String cover;

	private String content_location;

	

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAvatar() {
		return avatar;
	}

	public void setAvatar(String avatar) {
		this.avatar = avatar;
	}



	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getCreate_time() {
		return DateUtil.format(DateUtil.parse(create_time, "yyyyMMddHHmm"), "yyyy-MM-dd HH:mm:ss");
	}

	public void setCreate_time(String create_time) {
		this.create_time = create_time;
	}

	public String getCover() {
		return cover;
	}

	public void setCover(String cover) {
		this.cover = cover;
	}

	public String getContent_location() {
		return this.content_location;
	}

	public void setContent_location(String content_location) {
		this.content_location = content_location;
	}

}
