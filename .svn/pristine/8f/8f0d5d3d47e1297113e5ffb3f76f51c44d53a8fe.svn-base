package com.gobestsoft.api.modular.basic;

import lombok.Data;

/**
 * Create by gutongwei
 * 2018/5/15
 */
@Data
public class FileUpload {

    public FileUpload() {
    }

    public FileUpload(String originalName, String savePath, String accessUrl) {
        this.original_name = originalName;
        this.save_path = savePath;
        this.access_url = accessUrl;
    }

    /**
     * 文件显示名称
     */
    private String original_name;

    /**
     * 相对路径
     */
    private String save_path;

    /**
     * 资源访问路径
     */
    private String access_url;



}
