<style type="text/css">
    .paper {
        position:relative;
        width:100%;
        margin: 10px 0;
        min-height: 600px;
        background:#fff;
        border:1px solid #eee;
        float:left;
        box-shadow:0 0 5px rgba(0,0,0,0.27),0 0 20px  rgba(0,0,0,.1) inset;
    }
    .paper::after,.paper::before {
        content:'';
        position:absolute;
        bottom:6px;
        width:100px;
        height:1px;
        z-index:-1;
        box-shadow:0 2px 12px 5px rgba(0,0,0,.3);
    }
    .paper::after {
        left:4px;
        transform:rotate(-6deg);
    }
    .paper::before {
        right:4px;
        transform:rotate(6deg);
    }

    .myTable{
        width: 90%;
        margin: 20px auto;
        table-layout: fixed;
        border-collapse: collapse;
    }
    .myTable th,tr,td{
        border: 1px solid #333;
    }
    .myTable thead tr,th{
        border: 0px !important;
    }

    .myTable tbody td{
        height: 33px;
        line-height: 33px;
        padding: 5px;
        text-align: center;
        font-family: '楷体';

    }
    .exclude{

        border-top: 0px!important;
        border-top: 0px!important;
    }

    .bold{
        font-weight: bold;
        font-family: 'Microsoft YaHei';
    }
    .paper:hover .download_tag{
        display: inline-block;
    }
    .download_tag {

        display: inline-block;
        position: absolute;
        right: 0;
        top: 0;
        width: 140px;
        height: 50px;
        filter: alpha(Opacity=80);
        -moz-opacity: 0.5;
        opacity: 0.5;
        background-color: #333;
        color: #FFF;
        text-align: center;
        line-height: 50px;
        border-radius: 10px;
        display: none;
        cursor: pointer;
    }

    tr.bold td{
        font-weight: bold;
        font-family: 'Microsoft YaHei';
    }
    tr.overflow{
        overflow-wrap: break-word; word-break: break-all;
    }
    [v-cloak] {
        display: none;
    }
    .inline-block{
        display: inline-block;
        position: relative;
    }
</style>

