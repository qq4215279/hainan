package com.gobestsoft.common.modular.cms.model;

import lombok.Data;

/** 
	*@author  作者 : liqicheng
	*@date 创建时间：2017年11月27日 下午4:27:34 
	 */
@Data
public class Channel {
	
	public Channel() {
		super();
	}

	public Channel(int id, String name) {
		super();
		this.id = id;
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	/**
	 * 频道id
	 */
	private int id;
	
	/**
	 * 频道名称
	 */
	private String name;
	

}
